// 5) 사람
// 속성 : 나이, 몸무게, 혈압, 체온, 혈당, 맥박
// 행동 : 생일, 운동, 밥먹기

public class Human
{
    private int age;
    private int weight;
    private int pres;
    private int temp;
    private int dang;

    public int birth( int curage)
    {
        age++;
        return curage;
    }

    public void exersice( int curweight)
    {
        weight--;
        pres++;
    }

    public void eat()
    {
        weight++;
        temp++;
        dang++;
    }
}
